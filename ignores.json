{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Ignore recommended directories + workspace-hack if it exists",
  "ignorePaths": [
    "workspace-hack/**",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**",
    "**/examples/**",
    "**/__tests__/**",
    "**/test/**",
    "**/tests/**",
    "**/__fixtures__/**"
  ]
}
